if instance_exists(obj_player)
{
    if obj_player.state = state.normal
    {

        //Switch the selected item on the hud
        switch(keyboard_key)
        {
            case ord("1"):
            {
                current_hud_itemx = 0;
                current_hud_itemy = 0;
                current_hud_item = items_equipment_grid_box[# 0, 0];
                item_selection = 0;
                break;
            }
            
            case ord("2"):
            {
                current_hud_itemx = 1;
                current_hud_itemy = 0;
                current_hud_item = items_equipment_grid_box[# 1, 0];
                item_selection = 1;
                break;
            }
            
            case ord("3"):
            {
                current_hud_itemx = 0;
                current_hud_itemy = 1;
                current_hud_item = items_equipment_grid_box[# 0, 1];
                item_selection = 2;
                break;
            }
            
            case ord("4"):
            {
                current_hud_itemx = 1;
                current_hud_itemy = 1;
                current_hud_item = items_equipment_grid_box[# 1, 1];
                item_selection = 3;
                break;
            }
        }
        
        //Switch the selected ability on the hud
        
        if mouse_wheel_up() {ability_selection--};
        if mouse_wheel_down() {ability_selection++};
        
